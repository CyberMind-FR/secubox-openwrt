<%/* Layout */%>
<section class="page-section">
    <div class="page-header">
        <h1 class="page-title"><%= page.title %></h1>
        <% if (page.description) { %>
        <p class="page-description"><%= page.description %></p>
        <% } %>
    </div>
    
    <div class="page-content">
        <%- page.content %>
    </div>
</section>
