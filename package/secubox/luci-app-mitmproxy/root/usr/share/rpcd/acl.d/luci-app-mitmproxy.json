{
	"luci-app-mitmproxy": {
		"description": "Grant access to mitmproxy",
		"read": {
			"ubus": {
				"luci.mitmproxy": ["status", "settings", "alerts", "threat_stats"]
			},
			"uci": ["mitmproxy"]
		},
		"write": {
			"ubus": {
				"luci.mitmproxy": [
					"install",
					"start",
					"stop",
					"restart",
					"save_settings",
					"set_mode",
					"setup_firewall",
					"clear_firewall",
					"clear_alerts",
					"haproxy_enable",
					"haproxy_disable",
					"sync_routes"
				]
			},
			"uci": ["mitmproxy"]
		}
	}
}
